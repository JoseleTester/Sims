@model IEnumerable<DomesticUnit>
@foreach (var p in Model)
{
    <div class="card border-primary mb-3" style="max-width: 20rem;">

        <div class="card-body">
            <h4 class="card-title">@p.RoomNumber rooms   @p.BathroomNumber bathrooms</h4>
           
            <p class="card-text">@p.Description</p>

        </div>

    </div>





}